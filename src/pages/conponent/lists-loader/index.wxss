.listsLoader {
  width: 100%;
  height: 100%;
  position: relative;
}
.listsHead {
  position: fixed;
  top: 0rpx;
  width: 100%;
  height: 60rpx;
  border-top: 1rpx solid rgb(226, 222, 222);
  border-bottom: 1rpx solid rgb(226, 222, 222);
  z-index: 2;
  background-color: white;
}
.title {
  float: left;
  text-align: center;
  font-size: 30rpx;
  line-height: 51rpx;
}
.move {
  position: absolute;

  bottom: 0rpx;
  width: 30rpx;
  height: 10rpx;
  background-color: green;
}
.listBody {
  width: 100%;
  height: 100%;
}
.scrollItem {
  width: 100%;
  height: 100%;
}
